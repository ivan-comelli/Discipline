 {
    "version": 2,
    "builds": [
      { "src": "build/static/**", "use": "@vercel/static" },
      { "src": "build/index.html", "use": "@vercel/static" },
      { "src": "/api/.*", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/build/index.html" }
    ]
  }